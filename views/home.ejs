<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <meta name="google-site-verification" content="_R0-4-nBjFrLnBz-RP8cr52hKxRQZjmoCKjH5ftFJjk" />
    <meta name="msvalidate.01" content="DE3543F977324B6B30BE093173D15E63" />
    
    <!-- <link href="css/main.css" rel="stylesheet"/> -->
    <style>
      <%- myCss %>
    </style>
  </head>
  <body class="container-fluid no-padding" style="padding: 0px;">
    <%- include partials/header.ejs %>
      <div class="container-fluid no-padding">
          <div class="wrapper-outer">
            <div class="wrapper-inner">
              <div class="col-md-9 col-sm-8 col-xs-12 left-outer-container" style="padding-left: 0px;">
                <%- articlesTemplate %>
              </div>
              <div class="col-md-3 col-sm-4 hide-xs-screen no-padding right-outer-container" style="padding-right: 0px;">
                  <div class="scorecard-menu right-column-box hide-xs-screen">
                    <ul class="nav nav-tabs icon-menu-bar">
                      <li class="active action-icons hm-active-action-icon">
                        <i class="icon-clock" data-toggle="tab" href="#match-timings"></i>
                      </li>
                      <li class="action-icons">
                        <i class="icon-bookmark" data-toggle="tab" href="#book-marks"></i>
                      </li>
                      <li class="action-icons">
                        <i class="icon-monitor" data-toggle="tab" href="#notifications"></i>
                      </li>
                      <li class="action-icons">
                        <i class="icon-users" data-toggle="tab" href="#friends"></i>
                      </li>
                      <li class="action-icons">
                        <i class="icon-chart-bar" data-toggle="tab" href="#trending"></i>
                      </li>
                    </ul>
                    <div class="tab-content" id="right-widget-scroller">
                      <div id="match-timings" class="tab-pane fade in active">
                        <%- matchWidgetTemplate %>
                      </div>
                      <div id="book-marks" class="tab-pane fade">
                        <p>Bookmarks</p>
                      </div>
                      <div id="notifications" class="tab-pane fade">
                        <p>Notifications.</p>
                      </div>
                      <div id="friends" class="tab-pane fade in">
                        <p>Friends.</p>
                      </div>
                      <div id="trending" class="tab-pane fade in">
                        <%- trendingArticlesTemplate %>
                      </div>
                    </div>
                  </div>

                  <div class="right-column-box rankings-row">
                    <%- teamRankingTemplate %>
                  </div>
                  <br>
                  <div class="right-column-box rankings-row">
                    <%- playerRankingTemplate %>
                  </div>
              </div>
            </div>
          </div>
        </div>
    <%- include partials/footer.ejs %>

        <!-- <link rel="preload" href="css/custom-bootstrap.css" as="style" onload="this.onload=null;this.rel='stylesheet'">
        <noscript><link rel="stylesheet" href="css/custom-bootstrap.css"></noscript> -->
        <!-- TODO:: ADD CROSS BROWSER SUPPORT FOR PRELOAD -->

        <script rel="preload" as="script" src="webpack/timer.bundle.js"></script>
        <script rel="preload" as="script" src="webpack/timer.bundle.js.map"></script>
        <script rel="preload" as="script" src="js/lib/jquery-3.1.1.min.js"></script>
        <script rel="preload" as="script" src="js/lib/bootstrap.min.js"></script>
  </body>
</html>
