{
  "name": "express-app",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": "6.9.0",
    "npm": "3.10.8"
  },
  "author": "CG",
  "scripts": {
    "start": "npm run build && npm run watch",
    "build": "node ajax/scripts/build.js && npm run build-sass && npm run build-ts && npm run tslint && npm run copy-static-assets",
    "serve": "nodemon dist/server.js",
    "build-react": "node ajax/scripts/build.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve\"",
    "test": "jest --forceExit",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css && node-sass src/public/css/stories-desktop.scss dist/public/css/stories-desktop.css && node-sass src/public/css/stories-mobile.scss dist/public/css/stories-mobile.css && node-sass src/public/css/main-mobile.scss dist/public/css/main-mobile.css && node-sass src/public/css/app-downloads.scss dist/public/css/app-downloads.css && node-sass src/public/css/news-desktop.scss dist/public/css/news-desktop.css && node-sass src/public/css/news-mobile.scss dist/public/css/news-mobile.css && node-sass src/public/css/match-mobile.scss dist/public/css/match-mobile.css && node-sass src/public/css/match-desktop.scss dist/public/css/match-desktop.css && node-sass src/public/css/series-mobile.scss dist/public/css/series-mobile.css && node-sass src/public/css/series-desktop.scss dist/public/css/series-desktop.css",
    "watch-sass": "node-sass -w src/public/css/match-desktop.scss dist/public/css/match-desktop.css",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "copy-static-assets": "node copyStaticAssets.js",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/async": "^2.0.40",
    "@types/body-parser": "^1.16.2",
    "@types/compression": "0.0.33",
    "@types/dotenv": "^2.0.20",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.35",
    "@types/express-session": "0.0.32",
    "@types/jest": "^19.2.2",
