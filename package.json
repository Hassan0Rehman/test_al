    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "^8.0.1",
    "pusher-js": "^4.2.2",
    "raf": "3.4.0",
    "react": "^16.2.0",
    "react-animate-height": "^0.10.9",
    "react-custom-scrollbars": "^4.2.1",
    "react-dev-utils": "^4.2.1",
    "react-dom": "^16.0.0",
    "react-facebook": "^4.1.1",
    "react-highcharts": "^16.0.1",
    "react-lazyload": "^2.3.0",
    "react-pusher": "^0.1.1",
    "react-select": "^1.2.1",
    "react-share": "^1.19.0",
    "react-swipe": "^5.1.1",
    "redis": "^2.8.0",
    "redux": "^3.7.2",
    "request": "^2.78.0",
    "request-ip": "^2.0.2",
    "shelljs": "^0.7.7",
