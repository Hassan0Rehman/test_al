    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "expose-loader": "^0.7.3",
    "express": "^4.14.0",
    "express-flash": "^0.0.2",
    "express-minify": "^1.0.0",
    "express-minify-html": "^0.11.5",
    "express-session": "^1.14.2",
